.list_filter_object_data {
    width: 100%;
    float: left;
}

.list_filter_object_data li{
    margin: 0;
    float: left;
    width: 128px;
    height: 30px;
    line-height: 30px;
    text-align: center;
    color: #2c3e50;
    border-top: 1px solid #dfdfdf;
    position: relative;
}

.map_warning {
    background-color: #fff;
    padding: 22px 20px 0 20px;
}

.map_warning_title {
    font-size: 12px;
    color: #bdc3c7;
    margin-bottom: 10px;
}

.map_warning table {
    color: #333;
    width: 100%;
    border-collapse:collapse;
}
.map_warning table th, .map_warning table td {
    width: 80%;
    text-align: center;
    border: 1px solid #dfdfdf;
    padding: 2px;
    position: relative;
}
.map_warning table th:first-child, .map_warning table td:first-child{
    width: 20%;
}

.real_data {
    position: relative;
    width: 100%;
    text-align: center;
    margin-bottom: 20px;
}

.show_data {
    font-size: 12px;
}

.show_data ul li {
    position: relative;
    padding-left: 35px;
    width: 230px;
    margin-bottom: 10px;
    cursor: pointer;
    box-sizing:content-box;
    -moz-box-sizing:content-box;	/* Firefox */
    -webkit-box-sizing:content-box;	/* Safari */
}

.show_data ul li:hover {
    background: #f6f6f6;
}

.show_data ul li p {
    margin-bottom: 4px;
}
