.map_list {
    margin: 0 auto;
}

.list_content {
    width: 300px;
    height: 700px;
    float: left;
    background-color: #ECF0F1;
    overflow: scroll;
}

.map_list {
    background-color: #111;
    height: 700px;
    /*border: 1px solid #ccc;*/
}

.list_filter {
    /*margin-bottom: 10px;*/
}

.list_filter_ul {
    float: left;
    padding: 10px 15px;
    padding: 10px 10px\0;
}

.list_filter_ul li {
    color: #929DA5;
    font-size: 12px;
    float: left;
    width: 120px;
}

.list_filter_ul li:first-child {
    margin-right: 20px;
}
.list_filter_ul li label {
    display: inline!important;
    padding-bottom: 10px;
}
.list_filter_ul li input {
    border: none;
    height: 40px;
    width: 100px;
    padding: 0 14px;
    margin: 10px 0;
    box-sizing:content-box;
    -moz-box-sizing:content-box;	/* Firefox */
    -webkit-box-sizing:content-box;	/* Safari */
}

.list_filter_object {
    background: #fff;
    width: 258px;
    /*padding: 0 14px;*/
    /*margin-top: -14px;*/
}

.list_filter_object_open {
    background-image: url("../img/icon_list1.png");
    background-position: -34px 0;
    width: 9px;
    height: 5px;
    display: inline-block;
    position: absolute;
    right: -10px;
    top: 6px;
}

.list_filter_object_header {
    position: relative;
    padding: 10px;
}

.list_filter_object_header input {
    border: 1px solid #dfdfdf!important;
    margin-left: 25px;
    width: 70px!important;
    height: 20px!important;
}
.list_filter_object_header button {
    background-color: #eee;
    color: #666;
    border: none;
    width: 55px;
    height: 22px;
    cursor: pointer;
}
.list_filter_object_header button:hover {
    background-color: #09e;
    color: #fff;
}
.list_filter_object_header select {
    position: absolute;
    opacity: 0;
    left: 0;
}
.map_pagination {
    padding: 10px 0 30px;
    background: #fff;
    margin: 0 auto;
    font-size: 14px;
    text-align: center;
}
.map_pagination input {
    width: 22px;
    height: 22px;
    border: 1px solid #e7ecf0;
    font-size: 14px;
}
.map_pagination a{
    border: 1px solid #e7ecf0;
    padding: 2px 10px;
}
.map_pagination a:hover{
    border: 1px solid #e7ecf0;
    padding: 2px 10px;
}


