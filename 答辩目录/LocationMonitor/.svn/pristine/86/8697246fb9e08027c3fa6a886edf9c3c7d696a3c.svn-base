package com.swust.kelab.domain;

import java.util.Date;

public class AlertConfig {
	
	private int alertConfigID;		//自增主键
	private int userID;				//由哪个用户配置
	private Date configTime;		//配置时间，格式 yyyy-mm-dd hh:mm:ss；
	private int monitorType;		//对象类型，0-IMEI，1-IMSI
	private String imei;
	private String imsi;
	private int logicCondition;		//对象区域逻辑判断，与 - 或
	private int areaType;			//区域类型，0矩形，1圆形，2行政区域
	private String longitudeA;  	//构成矩形区域的经纬度
	private String longitudeB;
	private String latitudeA;
	private String latitudeB;
	private String longitudeCenter; //圆形区域的圆心经度
	private String latitudeCenter;	//圆心纬度
	private int radius;    			//圆心半径  
	private int areaID;				//行政区域标识
	private int pushFlag;			//是否推送，0-否，1-是
	
	public int getAlertConfigID() {
		return alertConfigID;
	}
	public void setAlertConfigID(int alertConfigID) {
		this.alertConfigID = alertConfigID;
	}
	public int getUserID() {
		return userID;
	}
	public void setUserID(int userID) {
		this.userID = userID;
	}
	public Date getConfigTime() {
		return configTime;
	}
	public void setConfigTime(Date configTime) {
		this.configTime = configTime;
	}
	public int getMonitorType() {
		return monitorType;
	}
	public void setMonitorType(int monitorType) {
		this.monitorType = monitorType;
	}
	public String getImei() {
		return imei;
	}
	public void setImei(String imei) {
		this.imei = imei;
	}
	public String getImsi() {
		return imsi;
	}
	public void setImsi(String imsi) {
		this.imsi = imsi;
	}
	public int getLogicCondition() {
		return logicCondition;
	}
	public void setLogicCondition(int logicCondition) {
		this.logicCondition = logicCondition;
	}
	public int getAreaType() {
		return areaType;
	}
	public void setAreaType(int areaType) {
		this.areaType = areaType;
	}
	public String getLongitudeA() {
		return longitudeA;
	}
	public void setLongitudeA(String longitudeA) {
		this.longitudeA = longitudeA;
	}
	public String getLongitudeB() {
		return longitudeB;
	}
	public void setLongitudeB(String longitudeB) {
		this.longitudeB = longitudeB;
	}
	public String getLatitudeA() {
		return latitudeA;
	}
	public void setLatitudeA(String latitudeA) {
		this.latitudeA = latitudeA;
	}
	public String getLatitudeB() {
		return latitudeB;
	}
	public void setLatitudeB(String latitudeB) {
		this.latitudeB = latitudeB;
	}
	public String getLongitudeCenter() {
		return longitudeCenter;
	}
	public void setLongitudeCenter(String longitudeCenter) {
		this.longitudeCenter = longitudeCenter;
	}
	public String getLatitudeCenter() {
		return latitudeCenter;
	}
	public void setLatitudeCenter(String latitudeCenter) {
		this.latitudeCenter = latitudeCenter;
	}
	public int getRadius() {
		return radius;
	}
	public void setRadius(int radius) {
		this.radius = radius;
	}
	public int getAreaID() {
		return areaID;
	}
	public void setAreaID(int areaID) {
		this.areaID = areaID;
	}
	public int getPushFlag() {
		return pushFlag;
	}
	public void setPushFlag(int pushFlag) {
		this.pushFlag = pushFlag;
	}
	
}
