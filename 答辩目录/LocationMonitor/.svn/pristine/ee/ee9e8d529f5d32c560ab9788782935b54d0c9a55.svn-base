.menu {
    width : 300px;
    background-color: #ECF0F1;
}
.menu ul {
    list-style-type : none;
}
.menu a {
    display : block;
    padding : 16px 20px;
    color : #666;
    text-decoration : none;
    width : 300px;
    background : #eaeaea;
    border-bottom : 1px solid #f1f1f1;
    text-align : center;
    text-transform : uppercase;
    font-family : "Arial Black", Gadget, sans-serif;
    font-size : 18px;
    font-weight : bold;
}
.menu a:hover {
     color : #222;
}
